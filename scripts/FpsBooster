_G.WaitPerAmount=_G.WaitPerAmount or 500;local a=game:GetService"Lighting"local b=workspace:FindFirstChildOfClass"Terrain"local c=game:GetDescendants()local d=_G.WaitPerAmount;local function e(a)if a:IsA"BasePart"and not a:IsA"MeshPart"then a.Material=Enum.Material.Plastic;a.Reflectance=0;a.CastShadow=false elseif a:IsA"Model"then a.LevelOfDetail=1 elseif a:IsA"MeshPart"then a.Reflectance=0;a.CastShadow=false;a.Material=Enum.Material.Plastic end end;if b then b.WaterWaveSize=0;b.WaterWaveSpeed=0;b.WaterReflectance=0;b.WaterTransparency=1;if sethiddenproperty then sethiddenproperty(b,"Decoration",false)end end;a.GlobalShadows=false;a.FogEnd=9e9;a.ShadowSoftness=0;if sethiddenproperty then sethiddenproperty(a,"Technology",3)end;settings().Rendering.QualityLevel=1;settings().Rendering.MeshPartDetailLevel=5;game.DescendantAdded:Connect(function(a)task.wait(_G.LoadedWait or 1)e(a)end)for a,b in ipairs(c)do e(b)if a%d==0 then task.wait()end end
