_G.WaitPerAmount=_G.WaitPerAmount or 500;local a=game:GetService("Lighting")local b=workspace:FindFirstChildOfClass("Terrain")local c=_G.WaitPerAmount;local function d(a)if a:IsA("BasePart")or a:IsA("MeshPart")then a.Material=Enum.Material.Plastic;a.Reflectance=0;a.CastShadow=false elseif a:IsA("ParticleEmitter")then a.Enabled=false;a.Rate=0 elseif a:IsA("Sparkles")or a:IsA("Smoke")or a:IsA("Fire")then a.Enabled=false end end;b.WaterWaveSize=0;b.WaterWaveSpeed=0;b.WaterReflectance=0;b.WaterTransparency=1;sethiddenproperty(b,"Decoration",false)a.GlobalShadows=false;a.ShadowSoftness=0;settings().Rendering.QualityLevel=1;settings().Rendering.MeshPartDetailLevel=5;game.DescendantAdded:Connect(function(a)task.wait(2)d(a)end)for a,b in ipairs(game:GetDescendants())do d(b)if a%c==0 then task.wait()end end
