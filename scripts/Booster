_G.WaitPerAmount=_G.WaitPerAmount or 500;local a=_G.WaitPerAmount;local b=game:GetService("Lighting")local c=workspace.Terrain;local function d(a)if a:IsA("BasePart")or a:IsA("MeshPart")then a.Material=Enum.Material.Plastic;a.Reflectance=0;a.CastShadow=false elseif a:IsA("ParticleEmitter")or a:IsA("Fire")or a:IsA("Smoke")then a.Enabled=false end end;if c then c.WaterWaveSize=0;c.WaterWaveSpeed=0;c.WaterReflectance=0;c.WaterTransparency=1;sethiddenproperty(c,"Decoration",false)end;b.GlobalShadows=false;b.ShadowSoftness=0;b.FogEnd=9e9;settings().Rendering.QualityLevel=1;settings().Rendering.MeshPartDetailLevel=5;local b=game:GetDescendants()local a=math.min(#b,a)for c=1,#b,a do for a=c,math.min(c+a-1,#b)do d(b[a])end;task.wait(0.01)end;game.DescendantAdded:Connect(function(a)task.delay(0.1,function()d(a)end)end)
