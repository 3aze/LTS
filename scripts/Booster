_G.WaitPerAmount=_G.WaitPerAmount or 500;local a,b=game:GetService"Lighting",workspace:FindFirstChildOfClass"Terrain"local c=_G.WaitPerAmount;local function d(a)if a:IsA"BasePart"and not a:IsA"MeshPart"then a.Material,a.Reflectance,a.CastShadow=Enum.Material.Plastic,0,false elseif a:IsA"Model"then a.LevelOfDetail=2 elseif a:IsA"MeshPart"then a.Material,a.Reflectance,a.CastShadow=Enum.Material.Plastic,0,false end end;if b then b.WaterWaveSize,b.WaterWaveSpeed,b.WaterReflectance,b.WaterTransparency=0,0,0,1;if sethiddenproperty then sethiddenproperty(b,"Decoration",false)end end;a.GlobalShadows,a.FogEnd,a.ShadowSoftness=false,9e9,0;if sethiddenproperty then sethiddenproperty(a,"Technology",3)end;settings().Rendering.MeshPartDetailLevel=5;game.DescendantAdded:Connect(function(a)task.wait(_G.LoadedWait or 1)d(a)end)for a,b in ipairs(game:GetDescendants())do d(b)if a%c==0 then task.wait()end end
